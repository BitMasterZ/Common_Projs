Attribute VB_Name = "Level2"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim W(0 To 20) As Integer, H(0 To 20) As Integer '10 movement positions
Dim intFrame(0 To 20) As Integer 'bitblt frames
Dim LeftPos As Integer, TopPos As Integer
Dim A As Integer 'Index for bitblt show
Dim Spare As Long, Q As Integer 'for arrays and spacing
Dim blnPress As Boolean 'TO prevent keydown procedure from repeating
Dim Index As Integer 'Collison
Dim moveX(0 To 20) As Integer ' Block movement
Dim T As Long
Dim Caller As Boolean, Over As Boolean, Win As Boolean
Dim Counter As Integer
Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
If Win = True Then Exit Sub

Select Case KeyCode
'Reiatsu charge
    Case vbKeyDown
                        
        Call Charge

'left/right movement
    Case vbKeyRight
        Call Movement(1)

    Case vbKeyLeft
        Call Movement(-1)

'Fly and fall
    Case vbKeyUp
        If blnPress = True Then Exit Sub
        blnPress = True

        Call Fly
        
'Position form
    Case vbKeyW
        Me.Top = Me.Top - 50
    
    Case vbKeyE
        Me.Left = Me.Left - 50
'Auto win
    Case vbKeyR

        MsgBox "You win"
        Win = True
        Level3.Show
        Unload Me
        
    Case vbKeyS
        Me.AutoRedraw = Not Me.AutoRedraw
        
        Case 27
        End


End Select
End Sub

Private Sub Charge()
If Win = True Then Exit Sub

If blnPress = True Then Exit Sub 'pressed more than once
blnPress = True
'Position and display
    TopPos = 580
    A = 1

Do Until intFrame(1) = 5

'Delay
    For Spare = -1000000 To 1000000
    Next Spare
    For Spare = -1000000 To 1000000
    Next Spare
    DoEvents
    
'Delay
    intFrame(1) = intFrame(1) + 1
Loop

blnPress = False

End Sub
Private Sub Form_KeyUp(KeyCode As Integer, Shift As Integer)
If Win = True Then Exit Sub
Select Case KeyCode

    Case vbKeyDown 'cancel charge

        Do Until intFrame(1) = 0
        'Delay
            DoEvents
            For Spare = -1000000 To 1000000
            Next Spare
            For Spare = -1000000 To 1000000
            Next Spare
            
        'return to normal pos
            intFrame(1) = intFrame(1) - 1
        Loop
End Select

End Sub

Private Sub Form_Load()
'Defaults
    'Dimensions
        W(1) = 100
        H(1) = 135
        
        W(0) = 145 / 1.1 + 2
        H(0) = 100 / 1.1

        W(2) = 143
        H(2) = 120
        
        W(3) = 50
        H(3) = 96

    For Spare = 0 To 10
        moveX(Spare) = 30
    Next
    
    Counter = 200
    
    Q = 5
    moveX(1) = 20
    TopPos = 640

End Sub
Private Sub Label1_Click()
    End
End Sub

Private Sub tmrmove_Timer()
If Win = True Then Exit Sub

Cls
'the slow firewall
    Counter = Counter - 1 'Time
    Label3 = Counter
    If Counter <= 0 Then Call GameOver
    
'bitblt function
    bitblt Me.hDC, LeftPos, TopPos, W(A), H(A), picMask(A).hDC, _
        intFrame(A) * W(A) + 4, 2 + Q, SRCAND
    bitblt Me.hDC, LeftPos, TopPos, W(A), H(A), picSprite(A).hDC, _
        intFrame(A) * W(A) + 4, 2 + Q, SRCINVERT
        
If Caller = True Then
    bitblt Me.hDC, LeftPos + W(A) / 3, TopPos - 10, W(3), H(3), picMask(3).hDC, _
        intFrame(3) * W(3) + 4, 2, SRCAND
    bitblt Me.hDC, LeftPos + W(A) / 3, TopPos - 10, W(3), H(3), picSprite(3).hDC, _
        intFrame(3) * W(3) + 4, 2, SRCINVERT

    If intFrame(3) < 7 Then
        intFrame(3) = intFrame(3) + 1
    Else
        Caller = False
        MsgBox "Karan died"
        TopPos = 600
        LeftPos = 10
        
        If Over = True Then
        MsgBox "Game over"
        End
        End If
    End If
    
End If
     
     If shpWall(12).Width > shpWall(3).Left Then Over = True
     
     Call FWmove
     Call Coll
 
End Sub

Private Sub Movement(W As Integer)
If Win = True Then Exit Sub
Select Case W

    Case 1 'Right
        If A <> 2 Then ' If not flying or falling
            A = 0
            TopPos = 641
        End If

        intFrame(0) = intFrame(0) + 1
        If intFrame(0) = 6 Then intFrame(0) = 0
        LeftPos = LeftPos + 10

    Case -1 'Left
        If A <> 2 Then
            A = 0
            TopPos = 641
        End If

        intFrame(0) = intFrame(0) - 1
        If intFrame(0) < 0 Then intFrame(0) = 5
        LeftPos = LeftPos - 10

End Select
End Sub

Private Sub Fly()
If Win = True Then Exit Sub
'Position
    A = 2
    Q = 3
    TopPos = 605

Do Until intFrame(2) = 3

'Delay
    For Spare = -2000000 To 2000000
    Next Spare
    For Spare = -2000000 To 2000000
    Next Spare
    
    DoEvents

    intFrame(2) = intFrame(2) + 1
Loop

Do Until TopPos <= 0

'Delay
    For Spare = -50000 To 50000
    Next Spare
    DoEvents

    intFrame(2) = intFrame(2) + 1
    If intFrame(2) > 6 Then intFrame(2) = 3
    TopPos = TopPos - 9
Loop

Call Fall
End Sub

Private Sub Fall()
If Win = True Then Exit Sub

Do Until TopPos >= 605

'Delay
    For Spare = -100000 To 100000
    Next Spare
    For Spare = -100000 To 100000
    Next Spare
    DoEvents

    intFrame(2) = intFrame(2) - 1
    If intFrame(2) < 3 Then intFrame(2) = 6
    TopPos = TopPos + 6

Loop

Do Until intFrame(2) = 0
    
'Delay
        For Spare = -2000000 To 2000000
    Next Spare
    For Spare = -2000000 To 2000000
    Next Spare
    For Spare = -2000000 To 2000000
    Next Spare
    DoEvents

    intFrame(2) = intFrame(2) - 1
Loop

blnPress = False

End Sub

Private Sub Coll()
If Win = True Then Exit Sub

For Spare = 0 To shpWall.UBound

    If TopPos + H(A) > shpWall(Spare).Top + shpWall(Spare).Height / 4 And TopPos < shpWall(Spare).Top + shpWall(Spare).Height - H(A) / 4 Then
        If LeftPos + W(A) > shpWall(Spare).Left + shpWall(Spare).Width / 4 And LeftPos < shpWall(Spare).Left + shpWall(Spare).Width - H(A) / 4 Then

            If Caller = True Then Exit Sub
            Caller = True
            If Spare = 12 Then Over = True
    
        End If
    End If

Next Spare

If TopPos + H(A) > shpExit.Top + shpExit.Height / 1.5 And TopPos < shpExit.Top + shpExit.Height - H(A) / 1.5 Then
    If LeftPos + W(A) > shpExit.Left + shpExit.Width / 1.5 And LeftPos < shpExit.Left + shpExit.Width - H(A) / 1.5 Then

    Call Form_KeyDown(vbKeyR, A)
   
    End If
End If

End Sub

Private Sub GameOver()
If Win = True Then Exit Sub
    
    Label3.Visible = False
    shpWall(12).Left = 0
    shpWall(12).Width = shpWall(12).Width + 1
    Label2(12).Left = shpWall(12).Left + shpWall(12).Width / 2

End Sub

Private Sub FWmove()
If Win = True Then Exit Sub

     shpWall(0).Top = shpWall(0).Top + moveX(0)
     If shpWall(0).Top >= 750 Then moveX(0) = -moveX(0)
     If shpWall(0).Top < 0 Then moveX(0) = -moveX(0)
     
     shpWall(1).Left = shpWall(1).Left - moveX(1)
     If shpWall(1).Left < 200 Then moveX(1) = -moveX(1)
     If shpWall(1).Left > 1000 Then moveX(1) = -moveX(1)
     
     shpWall(2).Left = shpWall(2).Left + moveX(2)
     If shpWall(2).Left < shpExit.Left Then moveX(2) = -moveX(2)
     If shpWall(2).Left > 1500 Then moveX(2) = -moveX(2)
     
     shpWall(10).Left = shpWall(10).Left - moveX(10)
     If shpWall(10).Left < -100 Then moveX(10) = -moveX(10)
     If shpWall(10).Left > shpExit.Left + shpExit.Width Then moveX(10) = -moveX(10)

End Sub
